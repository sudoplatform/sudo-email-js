{
  "name": "@sudoplatform/sudo-email",
  "version": "0.0.1",
  "author": "Anonyome Labs, Inc.",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/sudoplatform/sudo-email-js.git"
  },
  "scripts": {
    "prepare": "husky install",
    "lint": "npm-run-all --parallel lint:eslint lint:prettier",
    "lint:eslint": "eslint \"{bin,test,src}/**/*.{js,json,jsx,ts,tsx}\"",
    "lint:prettier": "prettier -l \"{bin,test,src}/**/*.{js,json,jsx,ts,tsx,yml}\"",
    "codegen": "graphql-codegen -c codegen.yml",
    "verify": "yarn audit-with-suppressions && yarn lint",
    "unit-test": "jest --verbose test/unit",
    "integration-test": "jest --verbose test/integration",
    "clean": "rm -rf ./docs ./lib ./cjs ./types",
    "build": "yarn codegen && yarn verify && yarn build:transpile && yarn build:docs",
    "build:transpile": "concurrently \"yarn build:transpile:declarations\" \"yarn build:transpile:cjs\" \"yarn build:transpile:esm\"",
    "build:transpile:declarations": "tsc --project tsconfig.declarations.json",
    "build:transpile:cjs": "tsc --project tsconfig.commonjs.json",
    "build:transpile:esm": "tsc",
    "build:docs": "typedoc --treatWarningsAsErrors --excludePrivate --out docs src/index.ts"
  },
  "main": "./cjs/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./cjs/index.js",
      "default": "./lib/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "cjs",
    "lib",
    "types",
    "docs"
  ],
  "peerDependencies": {
    "@sudoplatform/sudo-api-client": "^12.0.1",
    "@sudoplatform/sudo-common": "^8.0.1",
    "@sudoplatform/sudo-user": "^12.1.0"
  },
  "dependencies": {
    "@sudoplatform/sudo-web-crypto-provider": "^7.0.1",
    "apollo-cache-inmemory": "^1.6.6",
    "async-mutex": "^0.3.2",
    "aws-appsync": "4.1.7",
    "aws-sdk": "^2.1194.0",
    "fp-ts": "^2.12.2",
    "graphql": "^15.8.0",
    "io-ts": "^2.2.17",
    "lodash": "^4.17.21",
    "monocle-ts": "^2.3.12",
    "newtype-ts": "^0.3.5",
    "redux": "^4.1.2",
    "tslib": "^2.4.0"
  },
  "resolutions": {
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "node-fetch": "^2.6.7",
    "ansi-regex": "^5.0.1",
    "minimist": "^1.2.6",
    "got": "^11.8.5",
    "dset": "^3.1.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/typed-document-node": "^2.3.3",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-typed-document-node/apollo-client-2": "^1.0.0",
    "@sudoplatform/sudo-api-client": "^12.0.1",
    "@sudoplatform/sudo-common": "^8.0.1",
    "@sudoplatform/sudo-entitlements": "^14.0.0",
    "@sudoplatform/sudo-entitlements-admin": "^11.0.2",
    "@sudoplatform/sudo-profiles": "^15.0.0",
    "@sudoplatform/sudo-user": "^12.1.0",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.180",
    "@types/nodemailer": "^6.4.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "apollo-client": "^2.6.10",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-tree-shaking": "^1.10.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "nodemailer": "^6.7.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "prettier-plugin-organize-imports": "^3.0.3",
    "rimraf": "^3.0.2",
    "subscriptions-transport-ws": "^0.11.0",
    "ts-jest": "^27.1.3",
    "ts-mockito": "^2.6.1",
    "typedoc": "^0.23.10",
    "typescript": "^4.7.3",
    "uuid": "^8.3.2",
    "wait-for-expect": "^3.0.2",
    "websocket": "^1.0.34"
  }
}
