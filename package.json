    "build:docs": "typedoc --readme README.external.md --treatWarningsAsErrors --excludePrivate --out docs src/index.ts"
  },
  "main": "./cjs/index.js",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.js",
      "require": "./cjs/index.js",
      "default": "./lib/index.js"
